{
  "name": "vscode-plugin",
  "displayName": "vscode-plugin",
  "description": "example",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "doraemon.webview",
        "remark": "注册显示webview的command",
        "title": "Doraemon"
      },
      {
        "command": "doraemon.rightMenu",
        "remark": "注册右键菜单的command",
        "title": "Doraemon AI"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "doraemon.rightMenu",
          "remark": "当编辑框被选中时，显示右键菜单",
          "group": "AI"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "doraemon",
          "title": "Doraemon",
          "icon": "resources/doraemon.svg"
        }
      ]
    },
    "views": {
      "doraemon": [
        {
          "type": "webview",
          "id": "doraemon.webview",
          "name": "Doraemon"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.196",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "lodash": "^4.17.21"
  }
}
